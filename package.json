{
    "name": "react-table-util",
    "version": "0.0.11",
    "author": "Patrick Malleier <github@numero33.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/numero33/react-table-util/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/numero33/react-table-util.git"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist/react-table-util.esm.js",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "test:watch": "tsdx test --watch",
        "test:coverage": "tsdx test --coverage",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "size-limit": [
        {
            "path": "dist/react-table-util.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/react-table-util.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^4.10.2",
        "@testing-library/react-hooks": "^5.1.2",
        "@types/flat": "^5.0.1",
        "@types/react": "^17.0.5",
        "@types/react-dom": "^17.0.3",
        "husky": "^6.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "size-limit": "^4.10.2",
        "tsdx": "^0.14.1",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "flat": "^5.0.2"
    }
}
